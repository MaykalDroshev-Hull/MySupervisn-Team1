<Window x:Class="MySupervisn_Team1.SetMarks"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MySupervisn_Team1"
        mc:Ignorable="d" ResizeMode="CanMinimize"
        Title="SetMarks" Height="307.895" Width="300">
    <Grid >
        <StackPanel>
            <Label Content="Name"/>
            <TextBox Width="200" HorizontalAlignment="Left" Name="Name" Margin="5,5" />
            <Label Content="Module"/>
            <ListBox Name="Modules" Height="100"/>
            <Label Content="Mark(0-100)"/>
            <TextBox PreviewTextInput="TextBox_PreviewTextInput" Width="200" Margin="5,5" HorizontalAlignment="Left" />
            <StackPanel Orientation="Horizontal">
                <Label/>
                <Button x:Name="Go_Back" Content="Go back" Width="100" />
                <Label/>
                <Button x:Name="Set" Content="Accept" Width="100"/>

            </StackPanel>
           

        </StackPanel>
    </Grid>
</Window>
